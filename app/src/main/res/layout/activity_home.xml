<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:padding="50dp">

    <Button
        android:id="@+id/btn_get_docentes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/str_btn_get_docentes"
        android:onClick="go_to_get_docentes"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="200dp"
        />

    <Button
        android:id="@+id/btn_crear_docente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:text="@string/crear_docente"
        android:layout_below="@+id/btn_get_docentes"
        android:onClick="go_to_set_docente"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/btn_get_docentes"
        android:layout_alignStart="@+id/btn_get_docentes"
        android:layout_below="@+id/btn_crear_docente"
        android:layout_marginTop="28dp"
        android:onClick="go_to_get_estudiante"
        android:text="Button" />

</RelativeLayout>
